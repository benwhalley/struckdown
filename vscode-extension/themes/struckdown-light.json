{
  "name": "Struckdown Light",
  "type": "light",
  "colors": {
    "editor.background": "#fafafa",
    "editor.foreground": "#383a42",
    "editor.lineHighlightBackground": "#FFFDE7"
  },
  "tokenColors": [
    {
      "scope": [
        "variable.other.placeholder.struckdown",
        "punctuation.definition.placeholder.begin.struckdown",
        "punctuation.definition.placeholder.end.struckdown",
        "storage.type.struckdown",
        "punctuation.separator.type.struckdown",
        "keyword.operator.multiplier.struckdown",
        "meta.placeholder.struckdown"
      ],
      "settings": {
        "background": "#FFFF00",
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.other.template.struckdown",
        "punctuation.definition.template.begin.struckdown",
        "punctuation.definition.template.end.struckdown"
      ],
      "settings": {
        "background": "#00FF00",
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "System tag - content background",
      "scope": [
        "meta.tag.system.struckdown",
        "meta.content.system.struckdown"
      ],
      "settings": {
        "background": "#EDE9FE",
        "foreground": "#6B21A8"
      }
    },
    {
      "name": "System tag - opening and closing tags",
      "scope": [
        "entity.name.tag.system.struckdown",
        "meta.tag.system.struckdown punctuation.definition.tag.begin.struckdown",
        "meta.tag.system.struckdown punctuation.definition.tag.end.struckdown"
      ],
      "settings": {
        "background": "#8B5CF6",
        "foreground": "#FFFFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Header tag - content background (faded post-it)",
      "scope": [
        "meta.tag.header.struckdown",
        "meta.content.header.struckdown"
      ],
      "settings": {
        "background": "#FFFDF5",
        "foreground": "#8B7355"
      }
    },
    {
      "name": "Header tag - opening and closing tags",
      "scope": [
        "entity.name.tag.header.struckdown",
        "meta.tag.header.struckdown punctuation.definition.tag.begin.struckdown",
        "meta.tag.header.struckdown punctuation.definition.tag.end.struckdown"
      ],
      "settings": {
        "background": "#FEF3C7",
        "foreground": "#92400E",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Checkpoint tag - solid background",
      "scope": [
        "meta.tag.checkpoint.closed.struckdown",
        "meta.tag.checkpoint.self-closing.struckdown",
        "entity.name.tag.checkpoint.struckdown",
        "string.unquoted.checkpoint-name.struckdown"
      ],
      "settings": {
        "background": "#E0F2FE",
        "foreground": "#0369A1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Obliviate tag - solid background",
      "scope": [
        "meta.tag.obliviate.closed.struckdown",
        "meta.tag.obliviate.self-closing.struckdown",
        "entity.name.tag.obliviate.struckdown"
      ],
      "settings": {
        "background": "#FEE2E2",
        "foreground": "#B91C1C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Break tag",
      "scope": [
        "meta.tag.break.struckdown",
        "meta.tag.break.self-closing.struckdown",
        "entity.name.tag.break.struckdown"
      ],
      "settings": {
        "background": "#FFEDD5",
        "foreground": "#C2410C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Include tag",
      "scope": [
        "meta.tag.include.struckdown",
        "entity.name.tag.include.struckdown"
      ],
      "settings": {
        "background": "#CCFBF1",
        "foreground": "#0D9488",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Action function calls",
      "scope": [
        "entity.name.function.action.struckdown",
        "punctuation.definition.action.struckdown"
      ],
      "settings": {
        "foreground": "#4078F2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.begin.struckdown",
      "settings": {
        "foreground": "#0184BC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Django/Jinja tag delimiters",
      "scope": [
        "punctuation.definition.tag.begin.django.struckdown",
        "punctuation.definition.tag.end.django.struckdown"
      ],
      "settings": {
        "foreground": "#50A14F"
      }
    },
    {
      "name": "Django/Jinja keywords",
      "scope": "keyword.control.django.struckdown",
      "settings": {
        "foreground": "#A626A4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Django/Jinja variables",
      "scope": "variable.other.django.struckdown",
      "settings": {
        "foreground": "#50A14F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "System tag modifiers",
      "scope": "entity.other.attribute-name.struckdown",
      "settings": {
        "foreground": "#A626A4",
        "fontStyle": "italic"
      }
    }
  ]
}
